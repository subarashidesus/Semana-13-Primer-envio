Proceso sin_titulo
	Definir i, numero1 como entero;
	i <- 12;
	
	Mientras i > 2 Hacer
		Escribir "El valor de i: ", i;
		i <- i - 1;
	FinMientras
	
	Escribir "Ingrese un numero";
	Leer numero1;
	
	Si ( numero1 > 10 ) Entonces
		Escribir "Es un numero mayor que 10";
		
	FinSi
FinProceso
